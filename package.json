  "main": "index.js",
  "scripts": {
    "rollup": "rollup src/app.js -o _a-rollup.js -f iife -m && sorcery -i _a-rollup.js",
    "babel": "babel _a-rollup.js -o _b-babel.js --presets=@babel/preset-es2015 --source-maps && sorcery -i _b-babel.js",
    "uglify": "uglifyjs _b-babel.js -o _c-uglify.js -c -m --source-map url='_c-uglify.js.map' && sorcery -i _c-uglify.js",
    "build": "npm run rollup && npm run babel && npm run uglify"
  },
